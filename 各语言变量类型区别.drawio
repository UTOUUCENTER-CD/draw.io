        <mxCell id="EvxiiptyXCckD52X8nOG-189" value="&lt;font style=&quot;font-size: 15px;&quot;&gt;$$\begin{equation}&lt;br&gt;\omega = \log_\alpha(1 + \kappa\omega) \tag 1&lt;br&gt;\end{equation}$$&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=10;fontColor=#0000FF;strokeWidth=1;" parent="1" vertex="1">
          <mxGeometry x="899" y="1640" width="340" height="70" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-190" value="$$当\alpha = 10 时，\kappa = 2.302 58,&lt;br&gt;对任意数j有$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="913" y="1690" width="290" height="50" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-193" value="$$\begin{equation}&lt;br&gt;\alpha^j\omega = (1 + \kappa\omega)^J =1 + &lt;br&gt;\frac{j}{1}\kappa\omega +\frac{j(j-1)}{1\cdot2}&lt;br&gt;\kappa^2\omega^2 + \cdots. \tag 2&lt;br&gt;\end{equation}$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="1029" y="1730" width="320" height="100" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-194" value="$$令j = \frac{z}{\omega}而z为定数，则j等于无穷大，代入上式有：$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;strokeWidth=1;" parent="1" vertex="1">
          <mxGeometry x="843.5" y="1807" width="480" height="30" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-195" value="$$\begin{equation}&lt;br&gt;\alpha^z = \left(1 + \frac{\kappa z}{j}\right)^J = &lt;br&gt;1 + \frac{1}{1}\kappa z + \frac{j(j-1)}{1 \cdot 2} \frac{\kappa^2 z^2}{j^2}&lt;br&gt;+\frac{j(j-1)(j-2)}{1 \cdot 2 \cdot 3}\left(\frac{\kappa z}{j}\right)^3 + \cdots. \tag 9&lt;br&gt;\end{equation}$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="980" y="1813" width="540" height="100" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-196" value="$$但因j是无穷大，所以\frac{j-1}{j} = 1 , \frac{j-2}{j} = 1 等等，代入上式有：$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="866" y="1905" width="540" height="30" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-197" value="$$\alpha^z = 1 + \frac{\kappa z}{1} + &lt;br&gt;\frac{\kappa^2 z^2}{1 \cdot 2} + \cdots.$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="917.75" y="1950" width="300" height="50" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-198" value="$$令z = 1 有：$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="904" y="2000" width="140" height="30" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-199" value="$$\alpha = 1 + \frac{\kappa}{1} + &lt;br&gt;\frac{\kappa^2}{1 \cdot 2} + \cdots.$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="936" y="2030" width="270" height="50" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-200" value="$$上面说到\Psi = \kappa \omega,取\alpha为相应于&lt;br&gt;\kappa = 1的那个数，则由上式有：$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="955" y="2080" width="360" height="50" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-201" value="$$\alpha = 2.718 281 828 459 045 235 360 28$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;strokeWidth=1;" parent="1" vertex="1">
          <mxGeometry x="922.75" y="2120" width="350" height="30" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-202" value="$$为简单计，我们用符号e表示此数：&lt;br&gt;e = 2.718 281 828 459 045 235 360 28$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="1010.5" y="2150" width="300" height="50" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-203" value="$$它相应于\kappa = 1,而且e表示无穷级数&lt;br&gt;1+\frac{1}{1}+\frac{1}{1 \cdot 2}+\frac{1}{1 \cdot 2 \cdot 3}+&lt;br&gt;\frac{1}{1 \cdot 2 \cdot 3 \cdot 4}+\cdots之和。&lt;br&gt;$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;strokeWidth=1;" parent="1" vertex="1">
          <mxGeometry x="1025" y="2182" width="410" height="90" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-204" value="它是自然对数或称双曲对数的底。" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="910" y="2182" width="250" height="30" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-205" value="$$再看对数，由公式(9),令其中的a = e，从而 k = 1，有：$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="905" y="2255" width="420" height="30" as="geometry" />
        </mxCell>
        <mxCell id="EvxiiptyXCckD52X8nOG-206" value="$$e^z = \left(1 + \frac{z}{j}\right)^J = 1 + \frac{z}{1} +&lt;br&gt;\frac{z^2}{1 \cdot 2} \cdot \frac{j}{j} \cdot \frac{j-1}{j} + \cdots.$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000FF;" parent="1" vertex="1">
          <mxGeometry x="904" y="2288" width="440" height="50" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-1" value="" style="endArrow=none;dashed=1;html=1;rounded=0;strokeWidth=3;" parent="1" edge="1">
          <mxGeometry width="50" height="50" relative="1" as="geometry">
            <mxPoint x="20" y="1360" as="sourcePoint" />
            <mxPoint x="832" y="1360" as="targetPoint" />
          </mxGeometry>
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-2" value="&lt;b&gt;&lt;font color=&quot;#0000cc&quot; style=&quot;font-size: 13px;&quot;&gt;对数的一位早期创始者：“&lt;/font&gt;&lt;/b&gt;&lt;b&gt;&lt;font style=&quot;font-size: 13px;&quot; color=&quot;#0000cc&quot;&gt;尼古拉•麦卡托&lt;/font&gt;&lt;/b&gt;&lt;b&gt;&lt;font color=&quot;#0000cc&quot; style=&quot;font-size: 13px;&quot;&gt;”实质上是用积分来定义对数的。&lt;/font&gt;&lt;/b&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" parent="1" vertex="1">
          <mxGeometry x="19" y="1385" width="440" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-3" value="$$\begin{equation}&lt;br&gt;\int_0^x{\frac{dx}{1+x}} \tag 1 \\&lt;br&gt;\end{equation}$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="120" y="1410" width="200" height="60" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-4" value="$$牛顿接受了他的思想，并把自己最得意的二项级数用上，与麦卡托互相独立地得到了\log(1+x)$$" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="25.5" y="1480" width="580" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-5" value="的泰勒级数展开式，然后又用非常精巧的反演法，得到了" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="19" y="1510" width="350" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-6" value="$$e^x$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="332" y="1508" width="70" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-7" value="的幂级数展开式。" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="365" y="1509" width="130" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-8" value="牛顿也用同样的方式来研究：" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="470" y="1509" width="190" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-9" value="$$\arcsin x =&lt;br&gt;&amp;nbsp;\int_0^x{\frac{dx}{\sqrt{1-x^2}}}&amp;nbsp;&amp;nbsp;&lt;br&gt;\text {以及它的反函数$\sin x$}&lt;br&gt;$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="70" y="1530" width="210" height="80" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-12" value="一直到1715，泰勒（Robert Taylor）发表了“增量法及其逆”一文，才给出了我们现在知道的泰勒公式。所以，历史的发展与我们&lt;br&gt;现在的教科书有些矛盾，先有反函数" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="30" y="1605" width="750" height="40" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-13" value="$$\log (1+x) - \int_0^x {\frac{dx}{1+x}} \text {和&lt;br&gt;$\arcsin x = \int_0^x{\frac{dx}{\sqrt{1-x^2}}}&lt;br&gt;$} &lt;br&gt;$$" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="40.5" y="1635" width="290" height="80" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-14" value="的级数展开式，后来才有" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="25.5" y="1704" width="170" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-15" value="$$e^x \text {和} \sin x$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="140" y="1704" width="160" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-16" value="的级数展开式。" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="244" y="1704" width="110" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-18" value="$$\text{我们的出发点是：一个函数$y = y(x)$可以看作一个微分方程的柯西问题：}$$" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="867" y="1734" width="510" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-19" value="&lt;font style=&quot;font-size: 15px;&quot;&gt;$$\begin{cases} \frac{dy}{dx} = f(x,y) &lt;br&gt;\\ y(x_0) = y_0 \end{cases} \tag A$$&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="260" y="1694" width="270" height="50" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-20" value="之解来定义，这里" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="510" y="1704" width="140" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-21" value="$$y&lt;br&gt;$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="626" y="1691" width="50" height="50" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-22" value="$$\text {可以是一个函数，也可以是一个向量} y = (y_1,\cdots,y_n)$$" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="40" y="1750" width="470" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-23" value="当然&amp;nbsp; &amp;nbsp;也相应地应为一个向量" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="402" y="1750" width="220" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-24" value="$$f$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="421" y="1750" width="60" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-25" value="$$f = (f_1,\cdots,f_n)$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="584" y="1750" width="180" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-27" value="&lt;b&gt;常微分方程的柯西问题（A）的存在于唯一定理：&lt;/b&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="30" y="1795" width="350" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-28" value="&lt;font style=&quot;font-size: 13px;&quot;&gt;设（1）右边的向量值函数&lt;/font&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="50" y="1835" width="170" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-29" value="&lt;font style=&quot;font-size: 13px;&quot;&gt;$$f(x,y)$$&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="187" y="1836" width="80" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-30" value="&lt;font style=&quot;font-size: 13px;&quot;&gt;的每一个分量均定义在&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="243" y="1835" width="150" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-31" value="&lt;font style=&quot;font-size: 13px;&quot;&gt;$$\it \Omega$$&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="337" y="1836" width="110" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-32" value="&lt;font style=&quot;font-size: 13px;&quot;&gt;中，&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=15;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="389" y="1834" width="50" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-33" value="$$(x_0,y_0)\in \it \Omega$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="370" y="1835" width="180" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-34" value="$$\text{而且在$\it \Omega$ 的子集&lt;br&gt;$|x - x_0| \leq \alpha,|y - y_0| \leq b$}$$" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="510" y="1830" width="250" height="40" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-35" value="中" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="756" y="1835" width="40" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-36" value="$$\text {对$(x,y)$连续对$y$满足利普希茨条件，则（A）必有唯一解存在于$[x_0 - h,x_0+h]$中，这里}$$" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="30" y="1870" width="620" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-37" value="$$h = \min |a,\frac{b}{\it M}|,\it M = SUP_{\mid x - x_0 \mid \leq a_+\mid y-y_0 \mid \leq b} \mid f(x,y) \mid$$" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
          <mxGeometry x="39" y="1905" width="640" height="30" as="geometry" />
        </mxCell>
        <mxCell id="JizAkbYYsaQCN_tu9lTJ-39" value="&lt;b&gt;从连续变化的角度看：&lt;/b&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;fontSize=13;fontColor=#0000CC;" parent="1" vertex="1">
