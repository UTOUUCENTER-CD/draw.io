        <mxCell id="eaX9Ms1q94o2it5L9uL4-36" value="政治经济学" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;rotation=-90;" vertex="1" parent="1">
